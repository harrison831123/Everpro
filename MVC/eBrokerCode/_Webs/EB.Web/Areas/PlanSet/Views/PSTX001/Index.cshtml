            }
            if (OpenQueryDate != "") {
                //簽收回條截止日 < RUN佣日 < 佣酬明細開放查詢日期 < 發佣日期
                if (salReceiptDate >= salRunDate || salRunDate >= OpenQueryDate || OpenQueryDate >= salPayDate) {
