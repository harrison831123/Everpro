        function CheckFieldValue() {
            var flag = true;
            $(".required-field").each(function () {
                if ($(this).val() == "") {
                    appendMessage("業績年月、序號、RUN佣日期、發佣日期、簽收回條截止日；皆不可空白");
                    flag = false;
                    return false;
                }
            });
            if (flag == false) {
                return false;
            }

            //check productionYM style
            var patternYYMM = "^[0-9]{4}/[0-1]{1}[0-9]{1}$";
            var productionYM = $("#productionYM").val();

            if (productionYM.match(patternYYMM) == null) {
                appendMessage("業績年月 格式錯誤");
                return false;
            } else {
                if (isNaN(Date.parse($("#productionYM").val()))) {
                    appendMessage("業績年月 格式錯誤");
                    return false;
                }
            }
            //check sequence style
            if ($("#sequence").val() != "1" && $("#sequence").val() != "2") {
                appendMessage("序號 格式錯誤");
                return false;
            }
            //日期格式化
            var hrCloseDate = $("#hrCloseDate").val(); //預計人事關檔日期
            var adjDateTimeStr = $("#adjDateTimeStr").val();//調整起日
            var salReceiptDate = $("#salReceiptDate").val();//簽收回條截止日
            var adjDateTimeEnd = $("#adjDateTimeEnd").val();//調整迄日
            var salRunDate = $("#salRunDate").val();//RUN佣日期
            var OpenQueryDate = $("#OpenQueryDate").val(); //佣酬明細開放查詢日期
            var salPayDate = $("#salPayDate").val(); //發佣日期

            var CaseadjDateTimeStr = "";
            if (adjDateTimeStr != "") {
                //調整起日
                CaseadjDateTimeStr = $('#adjDateTimeStr').val() + " " + $('#ddlsHours option:selected').val() + ":" + $('#ddlsMinute option:selected').val();
            }
            var CaseadjDateTimeEnd = "";
            if (adjDateTimeEnd != "") {
                //調整迄日
                CaseadjDateTimeEnd = $('#adjDateTimeEnd').val() + " " + $('#ddleHours option:selected').val() + ":" + $('#ddleMinute option:selected').val();
            }

            //var CasesalReceiptDate = $('#salReceiptDate').val() + " " + "00:00";//簽收回條截止日

            //RUN佣日期、發佣日期、簽收回條截止日；皆不可空白
            //人事關檔 調整起日 調整迄日 佣酬明細開放查詢日期；可空白
            //人事關檔 ≦ 調整起日 ≦ 簽收回條截止日 ≦ 調整迄日 < RUN佣日 < 佣酬明細開放查詢日期 < 發佣日期

            //簽收回條截止日 < RUN佣日 < 發佣日期
            if (salReceiptDate >= salRunDate || salRunDate >= salPayDate) {
                appendMessage("預計人事關檔日期 ≦ 調整起日 ≦ 簽收回條截止日 ≦ 調整迄日 < RUN佣日 < 佣酬明細開放查詢日期 < 發佣日期");
                //console.log(1)
