        function GetReport() {
            var url = "@Url.Action("GetOpCalendarReport", "PSTX001", new { area = "PlanSet" })";
            var dataJson = $("form#QueryForm").serializeArray();
            let productionYM = $("#productionYM").val();
            $.extend(
                {
                    redirectPost: function (location, args) {
                        var form = '';
                        $.each(args, function (key, value) {
                            form += '<input type="hidden" name="' + value.name + '" value="' + value.value + '">';
                        });
                        form += '<input type="hidden" name="ProductionYM" value="' + productionYM + '">';
                        $('<form action="' + location + '" method="POST">' + form + '</form>').appendTo('body').submit();
                    }
                });

            $.redirectPost(url, dataJson);

        }
    </script>
