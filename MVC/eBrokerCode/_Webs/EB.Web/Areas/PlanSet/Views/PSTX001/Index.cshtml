                }
            }
            if (adjDateTimeEnd != "") {
                //簽收回條截止日 ≦ 調整迄日 < RUN佣日
                if (salReceiptDate > adjDateTimeEnd || adjDateTimeEnd >= salRunDate) {
                    appendMessage("預計人事關檔日期 ≦ 調整起日 ≦ 簽收回條截止日 ≦ 調整迄日 < RUN佣日 < 佣酬明細開放查詢日期 < 發佣日期");
                    //console.log(7)
