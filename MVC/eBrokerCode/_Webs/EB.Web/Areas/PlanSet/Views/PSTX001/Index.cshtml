        function DisassembleDateTime(fullTime) {
            var disassObj = new Object();
            disassObj.Date = fullTime.substr(0, fullTime.indexOf(' '));
            disassObj.Hour = fullTime.substr(fullTime.indexOf(':') - 2, 2);
            disassObj.Hour = fullTime.indexOf('PM') >=0 ? (parseInt(disassObj.Hour) + 12).toString() : disassObj.Hour;
            disassObj.Minute = fullTime.substr(fullTime.indexOf(':') + 1, 2);
            disassObj.Hour = disassObj.Hour == "" ? "00" : disassObj.Hour;
            disassObj.Minute = disassObj.Minute == "" ? "00" : disassObj.Minute;
            return disassObj;
        }

