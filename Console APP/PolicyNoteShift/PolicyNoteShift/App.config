<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<connectionStrings>	
		<add name="VisualBancas_EP" connectionString="Data Source= ;Initial Catalog=Visualbancas_EP;User ID=;pwd=" providerName="System.Data.SqlClient"/>
		<add name="VLIFE" connectionString="Data Source= ;Initial Catalog=VLIFE;User ID=;pwd=" providerName="System.Data.SqlClient"/>
		<add name="CUF" connectionString="Data Source=;Initial Catalog=CUF;Persist Security Info=True;User ID=;Password=" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<!--執行環境-->
		<add key="ENV" value="" />
		<!--執行Rerun Y:可重複執行-->
		<add key="RerunFlag" value="" />
		<!--執行Rerun的保公，多家用|分隔(009|003)-->
